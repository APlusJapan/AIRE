<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AIRE_App.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AIRE_App"
    Shell.FlyoutBehavior="Flyout"
    Title="AIRE_App">

    <FlyoutItem Title="FlyoutItem" Route="FlyoutItem">
        <Tab Title="Tab" Route="Tab">
            <ShellContent
                Title="ShellContent"
                ContentTemplate="{DataTemplate local:MainPage}"
                Route="MainPage" />
        </Tab>
    </FlyoutItem>

</Shell>
