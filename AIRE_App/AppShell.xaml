<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AIRE_App.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AIRE_App.Views"
    Shell.FlyoutBehavior="Flyout"
    Title="AIRE_App">

    <FlyoutItem FlyoutDisplayOptions="AsMultipleItems">
        <ShellContent
            Route="Index"
            Title="AI 不動産"
            ContentTemplate="{DataTemplate local:IndexView}"/>
        <ShellContent
            Route="AIChat"
            Title="AIチャット"
            ContentTemplate="{DataTemplate local:AIChatView}" />
    </FlyoutItem>

</Shell>
